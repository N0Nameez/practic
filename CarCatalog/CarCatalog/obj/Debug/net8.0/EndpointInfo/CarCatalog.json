            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          },
          "confirmPassword": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UpdateCarRequest": {
        "required": [
          "color",
          "modelId",
          "status"
        ],
        "type": "object",
        "properties": {
          "modelId": {
            "type": "integer",
            "format": "int32"
          },
          "vin": {
            "maxLength": 17,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "year": {
            "maximum": 2100,
            "minimum": 2000,
            "type": "integer",
            "format": "int32"
          },
          "color": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "price": {
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "mileage": {
            "maximum": 2147483647,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "status": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string"
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "photoUrl": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string",
            "format": "uri",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UpdateProfileRequest": {
        "required": [
          "currentPassword",
          "email"
